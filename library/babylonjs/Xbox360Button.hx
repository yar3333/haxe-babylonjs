package babylonjs;

/**
 * Defines supported buttons for XBox360 compatible gamepads
 */
@:native("BABYLON.Xbox360Button")
@:enum abstract Xbox360Button(Dynamic)
{
	/**
	 * A 
	 */
	var A = 0;
	/**
	 * B 
	 */
	var B = 1;
	/**
	 * X 
	 */
	var X = 2;
	/**
	 * Y 
	 */
	var Y = 3;
	/**
	 * Start 
	 */
	var Start = 4;
	/**
	 * Back 
	 */
	var Back = 5;
	/**
	 * Left button 
	 */
	var LB = 6;
	/**
	 * Right button 
	 */
	var RB = 7;
	/**
	 * Left stick 
	 */
	var LeftStick = 8;
	/**
	 * Right stick 
	 */
	var RightStick = 9;
}